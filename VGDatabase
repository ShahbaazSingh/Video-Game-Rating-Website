DROP TABLE IF EXISTS gameGenre;
CREATE TABLE gameGenre (
gameGenreID INT AUTO_INCREMENT,
gameID INT,
Genre VARCHAR(50),
PRIMARY KEY (gameGenreID)
);

LOCK TABLES gameGenre WRITE;

