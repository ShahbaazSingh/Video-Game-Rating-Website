

DROP TABLE IF EXISTS videoGame;
CREATE TABLE videoGame (
gameID INT AUTO_INCREMENT,
Title VARCHAR (100),
releaseDate date,
Developer VARCHAR (100),
Publisher VARCHAR (100),
Description VARCHAR (2000),
PRIMARY KEY (gameID)
);


LOCK TABLES videoGame WRITE;

